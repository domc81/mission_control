{
  "name": "workspace-cestra",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "convex:deploy": "node_modules/.bin/convex deploy --yes --typecheck disable --codegen enable --env-file .env.local",
    "ui:build": "cd ui && npm run build",
    "ui:deploy": "npm run ui:build && cp -r ui/dist/* /var/www/mission-control/",
    "deploy": "npm run convex:deploy && npm run ui:deploy"
  },
  "dependencies": {
    "convex": "^1.32.0"
  }
}
